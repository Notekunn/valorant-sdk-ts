{
  "name": "@notekunn/valorant-sdk",
  "version": "1.1.0",
  "description": "TypeScript API library for Riot login and Valorant client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "example": "env-cmd -f .env ts-node examples/cookie-authentication.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts examples/**/*.ts",
    "lint:fix": "eslint src/**/*.ts examples/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\" \"examples/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\" \"examples/**/*.{ts,js}\"",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "check": "pnpm run lint && pnpm run format:check && pnpm run test",
    "fix": "pnpm run lint:fix && pnpm run format"
  },
  "keywords": [
    "valorant",
    "riot",
    "api",
    "typescript",
    "gaming"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sort-keys": "^2.3.5",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "node-cache": "^5.1.2",
    "set-cookie-parser": "^2.7.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "packageManager": "pnpm@10.11.0"
}
